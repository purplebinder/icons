//= depend_on_asset "purplebinder.eot"
//= depend_on_asset "purplebinder.ttf"
//= depend_on_asset "purplebinder.svg"
//= depend_on_asset "purplebinder.woff"

@font-face {
	font-family: 'Purple Binder';
    src:url('<%= font_path('purplebinder.eot?-mvtcci') %>');
    src:url('<%= font_path('purplebinder.eot?#iefix-mvtcci') %>') format('embedded-opentype'),
        url('<%= font_path('purplebinder.woff?-mvtcci') %>') format('woff'),
        url('<%= font_path('purplebinder.ttf?-mvtcci') %>') format('truetype'),
        url('<%= font_path('purplebinder.svg?-mvtcci#icomoon') %>') format('svg');
	font-weight: normal;
	font-style: normal;
}

i.pb {
	font-family: 'Purple Binder';
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;

	/* Better Font Rendering =========== */
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}

i.pb.pb-binder:before {
	content: "\e600";
}

i.pb.pb-logo:before {
	content: "\e601";
}

